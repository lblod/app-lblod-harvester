{
  ?agendaPunt
    a besluit:Agendapunt ;
    owl:sameAs ?anotherAgendaPunt .
} => {
  ?anotherAgendaPunt
    a besluit:Agendapunt ;
    owl:sameAs ?agendaPunt .
}.

{
  ?agendaPunt
    owl:sameAs ?anotherAgendaPunt .

  ?anotherAgendaPunt
    a besluit:Agendapunt .
} => {
  ?anotherAgendaPunt
    owl:sameAs ?agendaPunt .

  ?agendaPunt
    a besluit:Agendapunt .
}.

{
  ?session
    besluit:behandelt ?agendaPunt .

  ?agendaPunt
    a besluit:Agendapunt ;
    owl:sameAs ?anotherAgendaPunt .
} => {
  ?session
    besluit:behandelt ?anotherAgendaPunt .
}.