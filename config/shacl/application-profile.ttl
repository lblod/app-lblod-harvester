          sh:datatype <http://www.w3.org/2001/XMLSchema#boolean> ;
          sh:minCount 1 ;
          sh:maxCount 1 ;
      ] ;
    sh:property [
          sh:name "gevolg" ;
          sh:description "Het gevolg van de stemming." ;
          sh:path <http://data.vlaanderen.be/ns/besluit#gevolg> ;
