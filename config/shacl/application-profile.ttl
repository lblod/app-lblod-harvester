          sh:minCount 1 ;
          sh:maxCount 1 ;
      ] ;
    sh:property [
          sh:name "onderwerp" ;
          sh:description "De beschrijving van het onderwerp waarover de stemming gaat." ;
          sh:path <http://data.vlaanderen.be/ns/besluit#onderwerp> ;
